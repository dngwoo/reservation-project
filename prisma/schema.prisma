datasource db {
  provider = "mysql"
  url = "mysql://root:poklpokl@localhost:3306/reservation"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  userId Int @default(autoincrement()) @id
  userName String
  email String
  password String
  mobileNumber String
  address String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

